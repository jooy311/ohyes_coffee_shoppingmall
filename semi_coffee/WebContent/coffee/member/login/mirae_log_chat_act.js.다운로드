if(typeof mi_push_data == "undefined"){
	mi_push_data = {};
	mi_push_data.set = function(){
		var sendData = {}
		sendData.fn = 'init';
		sendData.mi_chat_return = mi_chat_return;
		sendData.mi_chat_return.adkey = window.mi_chat_adkey;
		document.getElementById('mi-push-script-iframe').contentWindow.postMessage(JSON.stringify(sendData), '*' );	
	}
		/*mi_push_data.display = function(){
		document.getElementById('mi-push-script-iframe').style.display = "block";		
		document.getElementById('mi-push-iframe-box').style.opacity = "1";
	}*/
	mi_push_data.display = function(contentHeight,type,position){
		if(type == "tbbar"){
			document.getElementById('mi-push-iframe-box').style.setProperty("width", "100%", "important");
			document.getElementById('mi-push-iframe-box').style.setProperty("right", "0px", "important");
			document.getElementById('mi-push-iframe-box').style.setProperty("max-width", "unset", "important")
			if(position == "top"){
				document.getElementById('mi-push-iframe-box').style.setProperty("top", "0px", "important")
				document.getElementById('mi-push-iframe-box').style.setProperty("bottom", "unset", "important")
			}else{
				document.getElementById('mi-push-iframe-box').style.setProperty("bottom", "0px", "important")
				document.getElementById('mi-push-iframe-box').style.setProperty("top", "unset", "important")
			}
		}else if(type == "round"){
			if(position == "top"){
				document.getElementById('mi-push-iframe-box').style.setProperty("top", "20px", "important")
				document.getElementById('mi-push-iframe-box').style.setProperty("bottom", "unset", "important")
			}
		}else if(type == "alert"){
			document.getElementById('mi-push-iframe-box').style.setProperty("left", "50%", "important");
			document.getElementById('mi-push-iframe-box').style.setProperty("top", "50%", "important");
			document.getElementById('mi-push-iframe-box').style.setProperty("bottom", "unset", "important");
			document.getElementById('mi-push-iframe-box').style.setProperty("right", "unset", "important");
			if(window.innerWidth >= 480){
				document.getElementById('mi-push-iframe-box').style.setProperty("transform", "translate(-50%,-50%)", "important");
			}else{
				document.getElementById('mi-push-iframe-box').style.setProperty("transform", "translate(-50%, -50%) scale(0.8)", "important");
			}
		}
		document.getElementById('mi-push-script-iframe').height= contentHeight;
		document.getElementById('mi-push-script-iframe').style.display	= "block";
		document.getElementById('mi-push-iframe-box').style.setProperty("z-index", "1000000000", "important")
		document.getElementById('mi-push-iframe-box').style.opacity = "1";
	}
	mi_push_data.close = function(){
		document.getElementById('mi-push-iframe-box').style.setProperty("z-index", "-1", "important")
		document.getElementById('mi-push-script-iframe').style.display	= "none";
	}
	mi_push_data.fadein = function(el, time) {
		el.style.opacity = 0;
		var last = +new Date();
		var tick = function() {
			el.style.opacity = +el.style.opacity + (new Date() - last) / time;
			last = +new Date();
			if (+el.style.opacity < 1) {
				(window.requestAnimationFrame && requestAnimationFrame(tick)) || setTimeout(tick, 16);
			}
		};
		tick();
	}
}


(function() {
	window.mi_chat_adkey = window.mi_chat_adkey || "" ;
	var mi_talk_vi = mi_cookie_data.getCookie('mi_talk_vi');
	var mi_talk_vc = mi_cookie_data.getCookie('mi_talk_vc');
	var config = {};
	mi_url_data = {};
	mi_url_data.icon = {};
	mi_url_data.icon.default	= "https://miraetalk.com/mir_mt/img/ico_talk01.png";
	mi_url_data.log				= "//log.miraetalk.com" ;
	mi_url_data.response		= "//log1.toup.net/mirae_response_talk.php" ;
	mi_url_data.sms				= "//img.toup.net/Sms/new_sms_[type]_v2.html?bi=[bi]&bip=[bip]&bl=[bl]&adkey=[adkey]&agent=[agent]";
	mi_url_data.chat			= "https://miraetalk.com/chatClient_v2";
	mi_url_data.push			= "https://miraetalk.com/miraepushWindow";

	//document.body.clientWidth;screen.availWidth;window.innerWidth;
	var icon = {};
	icon.size = 60;
	icon.fontsize = 14;
	icon.dw = 500;
	icon.sw = window.innerWidth;
	icon.rate = 1;
	icon.addHeight = 0;
	if ( getDevice() == 'M' && icon.dw < icon.sw ){
		icon.rate = (icon.sw / icon.dw).toFixed(2);
		icon.size = (icon.size * icon.rate).toFixed(2);
		icon.fontsize = (icon.fontsize * icon.rate).toFixed(2);
	}

	var divs = {};
	divs.chat = "mi_chat_box";
	divs.sms = "mi_sms_box";

	function t(){
		if ( mi_chat_handledre.state ) return;
		mi_chat_handledre.state = true;
		if ( !window.mi_chat_adkey.length ) return;
		getLoginInfo();
		//mi_uk = k;
		if ( checkSSL() ){
			setTimeout(function(){ 
				checkMiRemove();
				callScript();
			}, 500);
		}
		//var interval_cnt = 0;
		//var interval = setInterval(function () {
			//interval_cnt++;
			////console.log(interval_cnt);
			//var k = getLoginInfo();
			//mi_uk = k;
			//if ( k != '' || interval_cnt >= 5 ) {
				//clearInterval(interval);
				//if ( checkSSL() ){
					//setTimeout(function(){ 
						//checkMiRemove();
						//callScript();
					//}, 500);
				//}
			//}
		//}, 300);
	}

	function checkSSL(){
		//return location.protocol=="https:" ? false: true;
		return true;
	}

	function checkMiRemove(){
		// defender layer img height get
		if ( document.getElementById("mirae_defender_layer") ){
			if ( _mi_defender_config !== 'undefined'){
				icon.addHeight = _mi_defender_config.is_yn == 'Y' && _mi_defender_config.position == 4  && typeof _mi_defender_config.imgH !== 'undefined' ? _mi_defender_config.imgH : 0 ;
			}
		}

		// sms layer remove
		//if ( document.getElementById("mirae_sms_layer") ) removeObjId('mirae_sms_layer');

		// btn layer remove
		//if (document.getElementById('mirae_btn_layer')) removeObjId('mirae_btn_layer');
	}

	function removeObjId(obj){
		document.getElementById(obj).parentNode.removeChild(document.getElementById(obj));
	}

	function callScript(){
		var ae = [];
		ae.push("?k=" + encodeURIComponent(window.mi_chat_adkey));
		ae.push("&u=" + getLocation());
		ae.push("&ru=" + getReferrer());
		ae.push("&mi_wm=" + getDevice());
		ae.push("&vi=" + mi_talk_vi);
		ae.push("&vc=" + mi_talk_vc);
		//ae.push("&uk=" + encodeURIComponent(mi_uk));
		ae.push("&a=" + fn_mi_brtype());
		ae.push(getUserInfo()) ;
		ae.push(getConvString()) ;
		ae.push(getSmsPageView());
		var url = ae.join("");
		var e = document.createElement("script");
		e.type = "text/javascript", e.async = !0, e.src = mi_url_data.response + url , e.charset = "UTF-8";
		var n = document.getElementsByTagName("script")[0];
		n.parentNode.insertBefore(e, n);
		e.onload = function() {
			getCall();
			e.onload = null;
			return
		};
		e.onerror = function() {
			console.log('no');
			e.onerror = null;
			return
		};
	}

	function getCall(){
		if ( typeof mi_chat_return === "object" && typeof mi_chat_return['flag'] === 'undefined'){
			config = mi_chat_return;
			if ( mi_objectCnt(config.log_data)) ctL();
			if ( mi_objectCnt(config.btn_list)) {
				getCommonAct();
				ctI();
				ctN();
			}
			if ( typeof config.sms === "object" && mi_objectCnt(config.sms)){
				ctSms();
			}
		}else{
			console.log(mi_chat_return);
		}
	}

	function getCommonAct(){
		ctC();
		ctS();
	}

