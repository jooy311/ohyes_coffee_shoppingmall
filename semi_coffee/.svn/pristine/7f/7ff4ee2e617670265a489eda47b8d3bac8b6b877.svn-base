package ohyes.coffee.dto;
/*
 Table QNA이(가) 생성되었습니다.

이름            널?       유형             
------------- -------- -------------- 
QNA_NO        NOT NULL NUMBER         
QNA_TITLE     NOT NULL VARCHAR2(500)  
QNA_MEM_ID    NOT NULL VARCHAR2(20)   
QNA_CONTENTS  NOT NULL VARCHAR2(4000) 
QNA_DATE      NOT NULL DATE           
QNA_READCOUNT NOT NULL NUMBER         
QNA_STATUS             NUMBER(1)      
QNA_CODE                NUMBER(1)      
*/
public class QnaDTO {
	private int qnaNo;
	private String qnaTitle;
	private String qnaMemId;
	private String qnaContents;
	private String qnaDate;
	private int qnaReadcount;
	private int qnaStatus;
	private int qnaCode;
	
	public QnaDTO() {
		// TODO Auto-generated constructor stub
	}

	public int getQnaNo() {
		return qnaNo;
	}

	public void setQnaNo(int qnaNo) {
		this.qnaNo = qnaNo;
	}

	public String getQnaTitle() {
		return qnaTitle;
	}

	public void setQnaTitle(String qnaTitle) {
		this.qnaTitle = qnaTitle;
	}

	public String getQnaMemId() {
		return qnaMemId;
	}

	public void setQnaMemId(String qnaMemId) {
		this.qnaMemId = qnaMemId;
	}

	public String getQnaContents() {
		return qnaContents;
	}

	public void setQnaContents(String qnaContents) {
		this.qnaContents = qnaContents;
	}

	public String getQnaDate() {
		return qnaDate;
	}

	public void setQnaDate(String qnaDate) {
		this.qnaDate = qnaDate;
	}

	public int getQnaReadcount() {
		return qnaReadcount;
	}

	public void setQnaReadcount(int qnaReadcount) {
		this.qnaReadcount = qnaReadcount;
	}

	public int getQnaStatus() {
		return qnaStatus;
	}

	public void setQnaStatus(int qnaStatus) {
		this.qnaStatus = qnaStatus;
	}

	public int getQnaCode() {
		return qnaCode;
	}

	public void setQnaCode(int qnaCod) {
		this.qnaCode = qnaCod;
	}
	
	

}
